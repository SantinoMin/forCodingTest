class Solution {
    public String solution(String rsp) {
        String answer = "";
        
        String[] c = rsp.split("");
        
        for(int i = 0; i < rsp.length(); i++){
            if(c[i].equals("2")){
                answer += "0";
            } else if(c[i].equals("0")){
                answer += "5";
            } else if(c[i].equals("5")){
                answer += "2";
            }
        }
                return answer;
    }
}