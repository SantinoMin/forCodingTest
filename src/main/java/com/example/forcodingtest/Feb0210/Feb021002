class Solution {
    public int solution(int angle) {
        int answer = 0;

        // if문으로 각도 angle숫자에 따라서 1~4중 해당하는 값을 반환하기

        if(angle > 0 && angle < 90){
            answer = 1;
        } else if(angle == 90){
            answer = 2;
        } else if(angle > 90 && angle < 180){
            answer = 3;
        } else if(angle == 180){
            answer = 4;
        }
        return answer;
    }
}